model{
  for (i in 1:n){
    y[i] ~ dnorm(mu[i], tau)
    mu[i] = gamma.0 + X[i, ] %*% gamma[1:df]
  }
  
  gamma[1:df] ~ dmnorm(init[1:df], Q[1:df,1:df])
  
  tau ~ dgamma(0.001, 0.001)
  lambda ~ dgamma(0.001, 0.001)
  
  gamma.0 ~ dnorm(0, 1e-6)

  for (j in 1:m){
  
    mu.rep[j] = gamma.0 + U[j, ] %*% gamma[1:df]
  
  }
  
}